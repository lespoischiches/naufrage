<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".coins.searchActivity.result.ResultActivity">




    <TextView
        android:id="@+id/titleResult"
        android:layout_width="246dp"
        android:layout_height="129dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="15dp"
        app:layout_constraintLeft_toRightOf="@id/return_button_result"
        tools:layout_editor_absoluteX="133dp" />

    <Button
        android:id="@+id/return_button_result"
        android:layout_width="108dp"
        android:text="Retour"
        android:layout_height="105dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:id="@+id/image_saved"
        android:layout_width="300dp"
        android:layout_height="221dp"
        android:layout_marginTop="68dp"
        app:layout_constraintTop_toBottomOf="@+id/titleResult"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteX="71dp" />

    <TextView
        android:id="@+id/destination"
        android:layout_width="300dp"
        android:layout_height="221dp"
        android:layout_marginTop="48dp"
        app:layout_constraintTop_toBottomOf="@+id/image_saved"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_editor_absoluteX="71dp" />


</androidx.constraintlayout.widget.ConstraintLayout>